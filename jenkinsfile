pipeline{
    agent any
    tools{
        maven 'm1'
    }
    stages {
  stage('checkout') {
    steps {
      checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'rajesh', url: 'https://github.com/Rajesh-jerubandi/Amazon-1.git']])
    }
  }
  stage('clean') {
    steps {
      sh 'mvn clean'
    }
  }
  stage('build') {
    steps {
      sh 'mvn install'
    }
  }

}

}
